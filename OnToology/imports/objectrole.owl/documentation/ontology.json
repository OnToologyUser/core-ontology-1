  "@id" : "http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl",
  "@type" : [ "http://www.w3.org/2002/07/owl#Ontology" ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements" : [ {
    "@value" : "What role does this object play? which objects do play that role?"
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#extractedFrom" : [ {
    "@value" : "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasConsequences" : [ {
    "@value" : "It is possible to make asser tions about roles, which are typically considered at the meta-level of an ontology. Instances of Role reify such elements, which are therefore put in the ordinar y domain of an ontology. It is not possible to parametrize the classiﬁcation over different dimensions e.g., time, space, etc."
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent" : [ {
    "@value" : "To represents objects and the roles they play."
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#isSpecializationOf" : [ {
    "@value" : "http://www.ontologydesignpatterns.org/cp/owl/classification.owl"
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#relatedCPs" : [ {
    "@value" : "It specializes the classiﬁcation CP. It is specialized by agent role."
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#scenarios" : [ {
    "@value" : "This old glass is used as a ﬂower pot."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "It is extracted from DOLCE Ultra Lite ontology and specialize the classification CP. Object is an Entity, and Role is a Concept. The hasRole relation specializes the classifies relation, while the isRoleOf specializes the isClassifiedBy relation."
  } ],
  "http://www.w3.org/2002/07/owl#imports" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/classification.owl"
  }, {
    "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl"
  } ],
  "http://www.w3.org/2002/07/owl#versionInfo" : [ {
    "@value" : "1.1 added rdfs:isDefinedBy for all named entities"
  }, {
    "@value" : "Created by Valentina Presutti"
  } ]
