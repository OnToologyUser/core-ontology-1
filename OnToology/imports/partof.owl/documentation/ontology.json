[ {
  "@id" : "http://www.ontologydesignpatterns.org/cp/owl/partof.owl",
  "@type" : [ "http://www.w3.org/2002/07/owl#Ontology" ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements" : [ {
    "@value" : "What is this entity part of? What are the parts of this entity?"
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasConsequences" : [ {
    "@value" : "This CP allows designers to represent entities and their parts i.e., part-whole relations, with transitivity. The temporal aspect of this relation cannot be expressed; in order to solve this issue the time indexed part of CP can be used."
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent" : [ {
    "@value" : "To represents entities and their parts."
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#relatedCPs" : [ {
    "@value" : "The time indexed part of is used as alternative when the temporal indexing has to be represented. Part of CP is specialized by componency. Sometimes this CP is wrongly used in order to model constituency and membership. These two modeling problems are addressed by constituency, and collection entity CPs, respectively."
  } ],
  "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#scenarios" : [ {
    "@value" : "Brain and heart are parts of the human body, substantia nigra is part of brain."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "part of"
  }, {
    "@value" : "part-whole"
  } ],
  "http://www.w3.org/2002/07/owl#imports" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl"
  } ],
  "http://www.w3.org/2002/07/owl#versionInfo" : [ {
    "@value" : "1.1 added rdfs:isDefinedBy for all named entities"
  }, {
    "@value" : "Created by Valentina Presutti"
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#TransitiveProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A transitive relation expressing parthood between any entities, e.g. 'the human body has a brain as part'. When specializing this pattern, take care of restricting the domain and range appropriately, since it could be counterintuitive to use this relation arbitrarily, e.g. between animals and planets. For an intransitive partof pattern, see 'componency'."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/partof.owl"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "has part"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  } ],
  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#TransitiveProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A transitive relation expressing parthood between any entities, e.g. 'brain is a part of the human body'. When specializing this pattern, take care of restricting the domain and range appropriately, since it could be counterintuitive to use this relation arbitrarily, e.g. between animals and planets. For an intransitive partof pattern, see 'componency'."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/partof.owl"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
